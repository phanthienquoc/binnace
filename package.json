{
  "name": "nodejs-express-template",
  "version": "1.0.0",
  "type": "module",
  "description": "Scalable and basic template for making Node.js apps by using express",
  "main": "index.ts",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier-eslint \"$PWD/{,!(node_modules)/**/}*.{js,json}\"",
    "format:fix": "prettier-eslint --write \"$PWD/{,!(node_modules)/**/}*.{js,json}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon --experimental-specifier-resolution=node --exec \"ts-node\" index.ts"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "17.1.1",
    "@types/cors": "^2.8.13",
    "@types/cron": "^2.0.0",
    "@types/lodash": "^4.14.191",
    "@types/node-telegram-bot-api": "^0.57.6",
    "bcrypt": "5.0.0",
    "body-parser": "1.19.0",
    "cors": "^2.8.5",
    "cron": "^2.1.0",
    "date-fns": "^2.29.3",
    "datejs": "^1.0.0-rc3",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-ip": "1.0.4",
    "firebase": "9.15.0",
    "firebase-admin": "^11.3.0",
    "firebaseui": "6.0.2",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "node-cron": "^3.0.2",
    "node-telegram-bot-api": "^0.60.0",
    "nodemon": "2.0.20",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "ws": "^8.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.15",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.2.5",
    "lint-staged": "10.2.9",
    "prettier": "2.0.5",
    "prettier-eslint-cli": "^5.0.0",
    "typescript": "^4.9.4"
  }
}